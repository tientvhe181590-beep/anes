{
    "properties": [
        {
            "name": "app.jwt.secret",
            "type": "java.lang.String",
            "description": "JWT signing secret key."
        },
        {
            "name": "app.jwt.access-token-expiry-ms",
            "type": "java.lang.Long",
            "description": "JWT access token expiry in milliseconds."
        },
        {
            "name": "app.jwt.refresh-token-expiry-ms",
            "type": "java.lang.Long",
            "description": "JWT refresh token expiry in milliseconds."
        },
        {
            "name": "app.gemini.api-key",
            "type": "java.lang.String",
            "description": "Google Gemini API key."
        },
        {
            "name": "app.gemini.model",
            "type": "java.lang.String",
            "description": "Gemini model name.",
            "defaultValue": "gemini-2.0-flash"
        },
        {
            "name": "app.vnpay.tmn-code",
            "type": "java.lang.String",
            "description": "VNPAY terminal/merchant code."
        },
        {
            "name": "app.vnpay.hash-secret",
            "type": "java.lang.String",
            "description": "VNPAY hash secret for signature verification."
        },
        {
            "name": "app.vnpay.pay-url",
            "type": "java.lang.String",
            "description": "VNPAY payment gateway URL.",
            "defaultValue": "https://sandbox.vnpayment.vn/paymentv2/vpcpay.html"
        }
    ]
}